                "@id": "dpla:dateRangeEnd"
            },
            "originalRecord": "dpla:originalRecord"
        },
        "fields": {
            "object": {
                "origin": "metadata/dc/identifier[2]",
                "processors": [
                    {
                        "process": "gsub",
                        "args": {
                            "pattern": "http:\\/\\/reflections.mndigital.org\\/u\\?\\/",
                            "replacement": ""
                        }
                    },
                    {
                        "process": "gsub",
                        "args": {
                            "pattern": ",",
                            "replacement": "&CISOPTR="
                        }
                    },
                    {
                        "process": "affix",
                        "args": {
                            "prefix": "http://reflections.mndigital.org/cgi-bin/thumbnail.exe?CISOROOT=/"
                        }
                    }
                ]
            },
            "isShownA": {
                "origin": "metadata/dc/identifier[2]"
            },
            "dataProvider": {
                "origin": "metadata/dc/publisher"
            },
            "provider": {
                "value": {
                    "@id": "http://dp.la/api/contributor/mdl",
                    "name": "Minnesota Digital Library"
                }
            },
            "title": {
                "origin": "metadata/dc/title",
                "processors": [
                    {
                        "process": "rstrip"
                    }
                ]
            },
            "sourceResource/title": {
                "origin": "metadata/dc/title",
                "processors": [
                    {
                        "process": "rstrip"
                    }
                ]
            },
            "sourceResource/spatial": {
                "origin": "metadata/dc/coverage",
                "processors": [
                    {
                        "process": "geonames_postal",
                        "args": [
                            "umntestuser"
                        ]
                    }
                ]
            },
            "sourceResource/description": {
                "origin": "metadata/dc/description"
            },
            "sourceResource/contributor": {
                "origin": "metadata/dc/contributor"
            },
            "sourceResource/format": {
                "origin": "metadata/dc/format",
                "processors": [
                    {
                        "process": "rstrip"
                    }
                ]
            },
            "sourceResource/rights": {
                "origin": "metadata/dc/rights"
            },
            "sourceResource/relation": {
                "origin": "metadata/dc/relation",
                "processors": [
                    {
                        "process": "rsplit",
                        "args": [
                            ";"
                        ]
                    },
                    {
                        "process": "rstrip"
                    }
                ]
            },
            "sourceResource/collection/title": {
                "origin": "setSpec/setDescription/oclcdc/title"
            },
            "sourceResource/collection/description": {
                "origin": "setSpec/setDescription/oclcdc/description"
            },
            "sourceResource/type": {
